<LoadDialog>:
    list: copy.copy(gv.dir_paths)

    BoxLayout:
		size: root.size
        pos: root.pos
        orientation: 'vertical'

        GridLayout:
            cols: 2

            BoxLayout:
                size: root.size
                pos: root.pos
                orientation: 'vertical'

                FileChooserIconView:
                    id: filechooser
                    dirselect: True
                    multiselect: True
                    path: gv.rootpath
                    on_selection: filechooser.selection = filechooser.selection if len(filechooser.selection) <= (8 - len(gv.dir_paths)) else gv.fs.limit(filechooser.path, filechooser.selection)

                BoxLayout:
                    size_hint_y: None
                    height: dp(35)
                    
                    Button:
                        text: 'Cancel'
                        font_name: 'assets/Montserrat-Regular'
                        on_release: root.cancel()
                        on_release: gv.dir_paths = root.list

                    Button:
                        id: add
                        disabled: False if len(gv.dir_paths) < 8 else True
                        text: 'Add'
                        font_name: 'assets/Montserrat-Regular'
                        on_release: gv.fs.add(filechooser.selection, root)

            BoxLayout:
                size_hint_x: None
                width: self.parent.width * .3
                orientation: 'vertical'

                StackLayout:
                    orientation: 'tb-lr'
                    padding: [dp(7.5), 0, 0, 0]
                    id: paths

                    FloatLayout:
                        size_hint: (None, None)

                        CheckBox:
                            id: 0
                            active: False if len(gv.dir_paths) == 0 else True
                            disabled: True if len(gv.dir_paths) == 0 else False
                            pos_hint: {'center_x': .3, 'center_y': .5}
                            on_press: gv.fs.remove(0, root)

                        Label:
                            id: label0
                            text: '' if len(gv.dir_paths) == 0 else os.path.normpath(os.path.basename(gv.dir_paths[0]))
                            size_hint_x: None
                            width: self.texture_size[0]
                            text_size: (dp(160), None)
                            pos_hint: {'x': .8, 'center_y': .5}

                    FloatLayout:
                        size_hint: (None, None)

                        CheckBox:
                            id: 1
                            active: False if len(gv.dir_paths) < 2 else True
                            disabled: True if len(gv.dir_paths) < 2 else False
                            pos_hint: {'center_x': .3, 'center_y': .5}
                            on_press: gv.fs.remove(1, root)

                        Label:
                            id: label1
                            text: '' if len(gv.dir_paths) < 2 else os.path.normpath(os.path.basename(gv.dir_paths[1]))
                            size_hint_x: None
                            width: self.texture_size[0]
                            text_size: (dp(160), None)
                            pos_hint: {'x': .8, 'center_y': .5}

                    FloatLayout:
                        size_hint: (None, None)

                        CheckBox:
