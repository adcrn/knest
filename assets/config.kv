<FolderSelectScreen>:
    on_enter: gv.fs = self
    on_leave: self.ids.path.text = "Please choose a directory"

    Button:
        size_hint: (.025, .035)
        pos_hint: {'center_x': .965, 'center_y': .967}
        background_normal: ''
        background_color: (0, 0, 0, 0)
        on_press: Factory.AdvancedSettings().open()

        Image:
            source: 'assets/settings.png'
            allow_stretch: False
            keep_ratio: True
            size: self.parent.size
            pos: self.parent.pos


    Button:
        size_hint: (.03, .04)
        pos_hint: {'center_x': .035, 'center_y': .967}
        background_normal: ''
        background_color: (0, 0, 0, 0)
        on_press: app.stop()

        Image:
            source: 'assets/quit.png'
            allow_stretch: False
            keep_ratio: True
            size: self.parent.size
            pos: self.parent.pos

    Button:
        size_hint: (.035, .045)
        pos_hint: {'center_x': .065, 'center_y': .967}
        background_normal: ''
        background_color: (0, 0, 0, 0)
        on_press: Window.minimize()

        Image:
            source: 'assets/minimize.png'
            allow_stretch: True
            keep_ratio: True
            size: self.parent.size
            pos: self.parent.pos

    BoxLayout:
        orientation: 'vertical'

        Label:
        	id: title
            font_size: dp(50)
            font_name: 'assets/Montserrat-Regular'
            text: "K N E S T"

        Image:
            size: self.size
            source: 'assets/color_bird.png'          

        Label:
        	id: path
        	font_size: dp(25)
            font_name: 'assets/Montserrat-Regular'
        	text: 'Please choose a directory'
            text_size: self.size
            halign: 'center'
            valign: 'middle'

        BoxLayout:
            orientation: 'horizontal'
            pos_hint: {'center_x': .5, 'bottom': 1}
            size_hint_y: None
            height: dp(60)

            Button:
                text: "Choose Folder"
                font_name: 'assets/Montserrat-Regular'
                on_release: root.show_load()

            Button:
                text: "Start"
                font_name: 'assets/Montserrat-Regular'
                on_release: root.check_path()

<AdvancedSettings@Popup>:
    auto_dismiss: False
    title: 'Advanced Settings'
    title_font: 'assets/Montserrat-Regular'
    title_size: sp(16)
    size_hint: (.4, .6)
    comp: gv.comp
    crop: gv.crop
    landscape: gv.landscape

    BoxLayout:
        orientation: 'vertical'

        FloatLayout:
            Label:
                id: caption1
                text: "REDUCE SIMILARITY"
                text_size: self.size
                halign: 'left'
                valign: 'middle'
                font_name: 'assets/Montserrat-Regular'
                font_size: dp(10)
                pos_hint: {'center_x': .6, 'center_y': .6}

            Switch:
                id: compare
                active: True if gv.comp else False
                size_hint_x: None
                size_hint_y: None
                # actual size of built-in kivy switch image
                size_x: dp(83)
                size_y: dp(32)
                pos_hint: {'center_x': .75, 'center_y': .6}
                on_active: gv.fs.update_compare(root)

            Label:
                id: caption2
                text: "CROP IMAGES"
                text_size: self.size
                halign: 'left'
                valign: 'middle'
                font_name: 'assets/Montserrat-Regular'
                font_size: dp(10)
                pos_hint: {'center_x': .6, 'center_y': .3}

            Switch:
                id: crop
                active: True if gv.crop else False
                size_hint_x: None
                size_hint_y: None
                # actual size of built-in kivy switch image
                size_x: dp(83)
                size_y: dp(32)
                pos_hint: {'center_x': .75, 'center_y': .3}
                on_active: gv.fs.update_crop(root)

            Label:
                id: caption3
                text: "LANDSCAPE STYLE"
                text_size: self.size
                halign: 'left'
                valign: 'middle'
                opacity: 1 if gv.crop else .5
                font_name: 'assets/Montserrat-Regular'
                font_size: dp(10)
                pos_hint: {'center_x': .6, 'center_y': 0}

            Switch:
                id: landscape
                active: True if gv.crop and gv.landscape else False
                disabled: False if gv.crop else True
                size_hint_x: None
                size_hint_y: None
                # actual size of built-in kivy switch image
                size_x: dp(83)
                size_y: dp(32)
                pos_hint: {'center_x': .75, 'center_y': 0}
                on_active: gv.fs.update_orientation(root)

        BoxLayout:
            orientation: 'horizontal'

            Button:
                text: 'Cancel'
                size_hint_y: None
                height: dp(25)  
                font_name: 'assets/Montserrat-Regular'
                on_release: gv.comp = root.comp
                on_release: gv.crop = root.crop
                on_release: gv.landscape = root.landscape
                on_release: root.dismiss()

            Button:
                text: 'OK'
                size_hint_y: None
                height: dp(25)  
                font_name: 'assets/Montserrat-Regular'
                on_release: root.dismiss()

